{
  "manifest_version": 2,
  "name": "Filter URL Extension",
  "version": "1.0",
  "description": "Fetch and analyze filter URLs",
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
